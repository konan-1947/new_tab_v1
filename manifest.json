{
  "manifest_version": 3,
  "name": "New Tab",
  "version": "1.0",
  "description": "Override the new tab page with a custom one.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["bookmarks"],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}